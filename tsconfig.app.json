{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["jasmine"]
  },
  "files": ["src/main.ts"],
  "include": [
    "src/**/*.d.ts",
    "src/Components/**/*.ts",
    "src/Services/**/*.ts",
    "src/app/**/*.ts"
